{
  "name": "vue3-typescript-starter",
  "version": "1.0.0",
  "description": "A modern Vue 3 starter template with TypeScript, Tailwind CSS, ESLint, and Prettier",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
