    "postcss": "^8.4.32",
    "sass-embedded": "^1.90.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite": "^6.4.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
